{
  "name": "app",
  "version": "3.0.0",
  "scripts": {
    "dev": "expo start --dev-client",
    "dev:android": "expo run:android --device",
    "dev:ios": "expo run:ios --device",
    "eas-build-pre-install": "chmod +x ./scripts/eas-build-pre-install.sh && ./scripts/eas-build-pre-install.sh",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "ci:eslint": "yarn eslint -f compact ./src --quiet",
    "ci:tsc": "yarn tsc --noEmit --skipLibCheck",
    "test": "jest",
    "release:android:build": "cd android && ./gradlew app:assembleRelease",
    "release:android:install": "cd android/app/build/outputs/apk/release && adb install app-release.apk",
    "dev:android:build": "cd android && ./gradlew app:assembleRelease",
    "dev:android:install": "cd android/app/build/outputs/apk/debug && adb install app-debug.apk",
    "ios:pod:annihilate": "cd ios && pod cache clean --all && pod deintegrate",
    "ios:pod:install": "cd ios && pod install",
    "eas:android:build": "eas build --platform android",
    "eas:ios:build": "eas build --platform ios",
    "eas:all:build": "eas build --platform all"
  },
